#MegaMain { background:#FFF7E0; }
QLabel#brand { color:#FFFFFF; font:800 20pt &quot;Pretendard&quot;; letter-spacing:0.5px; }
QLabel#mode_badge { color:#3A2D1F; background:#FFD866; border-radius:12px; padding:4px 10px; font:700 11pt &quot;Pretendard&quot;; }
QToolButton#back_btn { color:#FFFFFF; background:transparent; border:none; font:700 18pt; }
QToolButton#back_btn:hover { color:#FFD866; }

QTabWidget::pane { border:2px solid #E0D5C3; border-radius:12px; background:#FFFFFF; }
QTabBar::tab { padding:10px 16px; font:700 12pt &quot;Pretendard&quot;; color:#3A2D1F; background:#FFEAA6; border:2px solid #E0D5C3; border-bottom:none; border-top-left-radius:10px; border-top-right-radius:10px; margin-right:4px; }
QTabBar::tab:selected { background:#FFFFFF; }
QTabBar::scroller { width:0px; }
QTabBar QToolButton { width:0px; height:0px; border:none; padding:0; margin:0; }

QFrame#cart_bar { background:#FFF1AD; border:2px solid #E0D5C3; border-radius:16px; }
QLabel#cart_total { font:800 14pt &quot;Pretendard&quot;; color:#3A2D1F; }
QPushButton#to_order_check_btn { background:#3A2D1F; color:#FFFFFF; border-radius:16px; padding:12px 16px; font:800 14pt &quot;Pretendard&quot;; }
QPushButton#to_order_check_btn:hover { background:#291F15; }

QLabel#page_title { color:#3A2D1F; font:800 20pt &quot;Pretendard&quot;; }
QTableWidget#order_table { background:#FFFFFF; border:2px solid #E0D5C3; border-radius:12px; }
QPushButton#order_back_btn, QPushButton#order_next_btn,
QPushButton#pay_back_btn, QPushButton#pay_next_btn,
QPushButton#charge_done_btn {
  font:700 14pt &quot;Pretendard&quot;; border-radius:16px; padding:12px 16px; border:3px solid #B38B00; background:#FFFFFF; color:#2D2D2D;
}
QPushButton#order_next_btn, QPushButton#pay_next_btn, QPushButton#charge_done_btn { background:#3A2D1F; color:#FFFFFF; border-color:#D2A400; }
QPushButton#order_back_btn:hover, QPushButton#pay_back_btn:hover { background:#FFF3B0; border-color:#8C6A00; }

QFrame#pay_grid { background:#FFFFFF; border:2px solid #E0D5C3; border-radius:16px; }
QFrame#pay_grid QPushButton { background:#FFEAA6; border:2px solid #E0D5C3; border-radius:14px; padding:16px; font:800 14pt &quot;Pretendard&quot;; color:#3A2D1F; }
QFrame#pay_grid PushButton:hover { background:#FFD866; }

QLabel#amount_label { font:800 18pt &quot;Pretendard&quot;; color:#3A2D1F; }
QLabel#charge_msg { font:800 18pt &quot;Pretendard&quot;; color:#3A2D1F; }
QLabel#charge_tip { font:600 12pt &quot;Pretendard&quot;; color:#3A2D1F; }

